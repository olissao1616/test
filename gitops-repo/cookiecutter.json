{
  "repo_name": "myapp-gitops",
  "app_name": "myapp",
  "github_org": "bcgov-c",
  "licence_plate": "abcd12",
  "team_name": "{{ cookiecutter.licence_plate }}",
  "project_name": "{{ cookiecutter.licence_plate }}",
  "description": "GitOps repository for {{ cookiecutter.app_name }}",
  "web_api_name": "web-api",
  "web_app_name": "react-baseapp",
  "web_api_image": "ghcr.io/myorg/web-api",
  "web_app_image": "ghcr.io/myorg/react-baseapp",
  "web_api_port": "8080",
  "web_app_port": "3000",
  "enable_tilt": "yes",
  "tilt_default_env": "dev",
  "tilt_release_name": "{{ cookiecutter.app_name }}",
  "tilt_backend_workload": "{{ cookiecutter.project_name }}-{{ cookiecutter.web_api_name }}",
  "tilt_frontend_workload": "{{ cookiecutter.project_name }}-{{ cookiecutter.web_app_name }}",
  "enable_database": "yes",
  "database_name": "postgresql",
  "namespace_dev": "{{ cookiecutter.licence_plate }}-dev",
  "namespace_test": "{{ cookiecutter.licence_plate }}-test",
  "namespace_prod": "{{ cookiecutter.licence_plate }}-prod",
  "nginx_version": "15.11.0",
  "postgresql_version": "14.1.1",
  "aspnetcore_version": "5.5.0",
  "postgresqlha_version": "14.0.0",
  "reactbaseapp_version": "0.1.0",
  "webapicore_version": "0.1.0"
}
